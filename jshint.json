{
  "comments_ref_links":[
    "http://gotofritz.net/blog/geekery/sharing-jshint-settings-grunt/",
    "http://jshint.com/docs/options/"
  ],
  "options": {
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "sub": true,
    "undef": true,
    "unused": true,
    "boss": true,
    "eqnull": true,
    "node": true,
    "globals": {
      "_": true,
      "describe": true,
      "it": true,
      "expect": true
    }
  }
}
